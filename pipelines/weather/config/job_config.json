{
  "job_name": "weather_data_sync",
  "description": "Extract and load weather data from OpenWeatherMap to Delta Lake",
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "extract_weather",
      "description": "Extract weather data from OpenWeatherMap API",
      "notebook_task": {
        "notebook_path": "/Shared/weather/extract_weather",
        "base_parameters": {}
      },
      "timeout_seconds": 1800
    },
    {
      "task_key": "load_weather",
      "description": "Load weather data to Delta Lake table",
      "depends_on": [
        {
          "task_key": "extract_weather"
        }
      ],
      "notebook_task": {
        "notebook_path": "/Shared/weather/load_weather",
        "base_parameters": {}
      },
      "timeout_seconds": 1800
    }
  ],
  "timeout_seconds": 7200,
  "tags": {
    "environment": "production",
    "pipeline": "weather",
    "data_source": "openweathermap"
  }
}
